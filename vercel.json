{
  "buildCommand": "cd frontend && npx vite build",
  "installCommand": "npm install && npm install --prefix frontend",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/graphql",
      "destination": "/api/graphql"
    }
  ],
  "functions": {
    "api/graphql.js": {
      "maxDuration": 30
    }
  }
}