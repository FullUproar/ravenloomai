  "buildCommand": "npm --prefix frontend run build",
  "installCommand": "npm install && npm --prefix frontend install",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/graphql",
      "destination": "/api/graphql"
    }
  ],
  "functions": {
    "api/graphql.js": {
      "maxDuration": 30
    }
  }
}